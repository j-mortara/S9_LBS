<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated smartHome -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="smartHome.presentation.SmartHomeModelWizardID"
            name="%_UI_SmartHomeModelWizard_label"
            class="smartHome.presentation.SmartHomeModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/SmartHomeModelFile.gif">
         <description>%_UI_SmartHomeModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated smartHome -->
      <editor
            id="smartHome.presentation.SmartHomeEditorID"
            name="%_UI_SmartHomeEditor_label"
            icon="icons/full/obj16/SmartHomeModelFile.gif"
            extensions="smarthome"
            class="smartHome.presentation.SmartHomeEditor"
            contributorClass="smartHome.presentation.SmartHomeActionBarContributor">
      </editor>
   </extension>

</plugin>
